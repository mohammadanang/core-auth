{
    "name": "apollo16/core-auth",
    "description": "apollo16 auth implementation",
    "require": {
        "illuminate/auth": "5.1.*",
        "apollo16/core-contracts": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Apollo16\\Core\\Auth\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "league/oauth2-client": "Required to use with the apollo16 authentication server"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
